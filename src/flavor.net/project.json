{
	"title": "Flavor",
	"version": "1.0.0",
	"description": "Flavor is a library for parsing and manipulating FLV files from .NET.",
	"authors": [ "James Ko" ],
	"owners": [ "James Ko" ],
	"tags": [
		".flv",
		"flv",
		"flvs",
		"file",
		"files",
		"flavor",
		"flash",
		"video",
		"videos",
		".net",
		"c#",
		"csharp"
	],
	"projectUrl": "https://github.com/jamesqo/flavor.net",
	"licenseUrl": "https://github.com/jamesqo/flavor.net/blob/master/license.bsd",

	"dependencies": {
		"Be.IO": "1.0.0"
	},

	"frameworks": {
		".NETPortable,Version=v4.5,Profile=Profile259": {
			"frameworkAssemblies": {
				"System.Collections": { "type": "build" },
				"System.IO": { "type": "build" },
				"System.Linq": { "type": "build" },
				"System.Runtime": { "type": "build" }
			}
		}
	},

	"compilationOptions": {
		"allowUnsafe": true,
		"optimize": true
	}
}
